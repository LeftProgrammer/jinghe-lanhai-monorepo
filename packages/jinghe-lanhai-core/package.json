{
	"name": "jinghe-lanhai-core",
	"version": "1.0.0",
	"description": "",
	"author": "",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "types/index.d.ts",
	"exports": {
	  ".": {
		"types": "./types/index.d.ts",
		"import": "./dist/index.mjs",
		"require": "./dist/index.cjs"
	  },
	  "./*": "./*"
	},
	"files": [
	  "dist",
	  "types",
	  "README.md"
	],
	"scripts": {
	  "dev": "pnpm --parallel vite",
	  "dev-types": "vue-tsc --noEmit -p tsconfig.app.json --watch",
	  "build": "pnpm build-lib && pnpm build-types",
	  "build-lib": "vite build",
	  "build-types": "vue-tsc --emitDeclarationOnly --declaration --noEmit false -p tsconfig.app.json",
	  "test": "vitest",
	  "test-ci": "vitest --run",
	  "release": "pnpm run build && pnpm run build-types && pnpm publish --access public"
	},
	"peerDependencies": {
	  "vue": "^3.4.21"
	},
	"dependencies": {
	  "axios": "^1.6.8",
	  "dayjs": "^1.11.10",
	  "lodash-es": "^4.17.21",
	  "pinia": "^2.1.7"
	},
	"devDependencies": {
	  "vite": "^5.2.6",
	  "vue-tsc": "^2.0.7",
	  "typescript": "^5.4.3",
	  "vitest": "^0.34.6"
	}
  }
  